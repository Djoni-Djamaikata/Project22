@model IEnumerable<Shop11A.Models.Product>




<div class="container-fluid d-flex align-items-center justify-content-center">
		</div>


<h1 class="text-center">Motorex</h1>


    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="/images/slide-1.jpg" class="d-block w-100" alt="...">
                <div class="carousel-caption d-none d-md-block">
                    <h2 class="boosted-elements-slide-title boosted-animation-delay-none"><span>DIRT <strong style="color:#ffffff">BIKE</strong> EVENT</span></h2><div class="boosted-elements-slide-sub-title boosted-animation-delay-none"><span>OFF ROAD</span></div><div class="boosted-elements-slide-content boosted-animation-delay-none"><span>Join us every month for our famous motocross bike races.<br>We hold races on the third Friday of the month. </span></div> 
                </div>
            </div>
            <div class="carousel-item">
                <img src="/images/slide-2.jpg" class="d-block w-100" alt="...">
                <div class="carousel-caption d-none d-md-block">
                <h2 class="boosted-elements-slide-title boosted-animation-delay-none"><span>NEW TO  MOTOCROSS</span></h2><div class="boosted-elements-slide-sub-title boosted-animation-delay-none"><span>RIDER CLASS</span></div><div class="boosted-elements-slide-content boosted-animation-delay-none"><span>We offer new rider classes every week. These courses are for all types of riders<br> from newbies to professional racers.</span></div>
            </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>



